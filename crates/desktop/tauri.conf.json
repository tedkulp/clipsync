{
  "productName": "ClipSync",
  "version": "0.1.0",
  "identifier": "com.clipsync.desktop",
  "build": {
    "beforeDevCommand": "npm --prefix ../../desktop run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../desktop/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    }
  },
  "app": {
    "windows": [
      {
        "title": "ClipSync",
        "width": 600,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "label": "main"
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  }
}
